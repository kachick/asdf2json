{
  "typescript": {
    "quoteStyle": "preferSingle",
    "useBraces": "always",
    "module.sortImportDeclarations": "maintain",
    "module.sortExportDeclarations": "maintain",
    "importDeclaration.sortNamedImports": "maintain",
    "exportDeclaration.sortNamedExports": "maintain"
  },
  "json": {},
  "markdown": {},
  "prettier": {
    "printWidth": 120,
    "singleQuote": true
  },
  "includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json,md,yml}"],
  "excludes": ["**/node_modules", "**/*-lock.json", "dist/**"],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.82.0.wasm",
    "https://plugins.dprint.dev/json-0.17.0.wasm",
    "https://plugins.dprint.dev/markdown-0.15.2.wasm",
    "https://plugins.dprint.dev/prettier-0.13.0.json@dc5d12b7c1bf1a4683eff317c2c87350e75a5a3dfcc127f3d5628931bfb534b1"
  ]
}
